{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Curso</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nota</th>
                <td>{{ entity.nota }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('curso') }}">
            Volver a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('curso_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
