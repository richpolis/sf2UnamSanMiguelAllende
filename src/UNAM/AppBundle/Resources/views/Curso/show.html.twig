{% extends 'UNAMAppBundle::layout.html.twig' %}

{% block content %}
    <h1>Curso</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombrecurso</th>
                <td>{{ entity.nombreCurso }}</td>
            </tr>
            <tr>
                <th>Nota</th>
                <td>{{ entity.nota }}</td>
            </tr>
            <tr>
                <th>Isingles</th>
                <td>{{ entity.isIngles }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cursos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cursos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
